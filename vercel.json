{
  "version": 2,
  "functions": {
    "api/stats.js": { "runtime": "nodejs18.x" },
    "api/track.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/love/(.*)", "dest": "/love/$1" },
    { "src": "/color/(.*)", "dest": "/color/$1" },
    { "src": "/quiz/(.*)", "dest": "/quiz/$1" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
