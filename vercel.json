{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },


    {
      "src": "/love",
      "dest": "/love/index.html"
    },
    {
      "src": "/love/(?<path>.*)",
      "dest": "/love/(?<path>.*)"
    },

    {
      "src": "/color",
      "dest": "/color/index.html"
    },
    {
      "src": "/color/(?<path>.*)",
      "dest": "/color/(?<path>.*)"
    },

    {
      "src": "/quiz",
      "dest": "/quiz/index.html"
    },
    {
      "src": "/quiz/(?<path>.*)",
      "dest": "/quiz/(?<path>.*)"
    },

    {
      "src": "/personality",
      "dest": "/personality/index.html"
    },
    {
      "src": "/personality/(?<path>.*)",
      "dest": "/personality/(?<path>.*)"
    },


    {
      "src": "/",
      "dest": "/index.html"
    },


    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
